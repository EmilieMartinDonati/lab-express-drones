<h2>Update the drone</h2>
<div class="row">
    <div class="col-12">
        <form action="/drones/create" method="GET"></form>
        <label for="name">Name</label>
        <input type="text" id="name" name="name" required value={{this.name}} />
        <label for="propellers">Number of propellers:</label>
        <input type="number" id="propellers" name="propellers" value={{this.propellers}}/>
        <label for="maxSpeed">MaxSpeed</label>
        <input type="number" id="maxSpeed" name="maxSpeed" value={{this.maxSpeed}}/>
        <button>Create a drone</button>
        </form>
    </div>
</div>