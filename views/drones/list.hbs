<h2>Available drones</h2>
{{#each drones}}
<div class="card">
	        <p>{{this.name}}</p>
            <p>{{this.propeller}}</p>
            <p>{{this.maxSpeed}}</p>
			<a href="/drones/{{this._id}}/edit">More info...</a>
			<form action="/drones/{{this._id}}/delete" method="post">
				<button>Delete</button>
			</form>
			<a href="/drones/{{this._id}}/edit">Update</a>
		</div>
{{/each}}